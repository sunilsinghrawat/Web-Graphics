<script>
	
	//var newDiv = "<div class='newDiv'><ul><li>One</li><li>Two</li></ul></div>"
	var newDiv = "<div class='newDiv'>";
	newDiv += '<ul><li>One</li><li>Two</li></ul>'
	newDiv +="</div>"
	var divVar = document.getElementsByTagName("div")[0].innerHTML = newDiv;
	

</script>
