<script>
	const abc = new Swiper('.swiper', {
	  slidesPerView: 1,

	  pagination: {
        el: ".swiper-pagination",
        clickable: true,
      },

	  // Navigation arrows
	  navigation: {
		nextEl: '.swiper-button-next',
		prevEl: '.swiper-button-prev',
	  },

