$(document).ready(function(){
    $("#pastEventsLink").on('click', function() {
      $("html, body").animate({
        scrollTop: ($('#pastEvents').offset().top - 10) + 'px'
      }, 1000);
    });
});
======================
======================
$(".knowMoreBtn").click(function() {
	var dest = $('#knowMoreRedirect').offset().top - 60;
	$('html,body').animate({
	scrollTop: dest
	}, 1500, 'swing');
}); 
